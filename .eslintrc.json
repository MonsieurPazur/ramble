{
    "env": {
		"node": true,
        "browser": true,
        "commonjs": true,
        "es6": true,
		"jquery": true
    },
    "extends": "airbnb-base",
	"parser": "babel-eslint",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
		"no-underscore-dangle": "off"
    }
}
